#include <stdio.h>
#include <stdlib.h>

nozeroinit struct H {
	int xh;
	void H();
};

struct A {
	int xa;
	H h;
};

nozeroinit struct B : A {
	int g;
};

void H::H(void)
{
	xh = 1;
}

void A::A(void)
{
	h.H();
	xa = 2;
}

typedef B C;

int main(void)
{
	C c;
	printf("%d %d\n", c.xa, c.h.xh);
	return 0;
}

